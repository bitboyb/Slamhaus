{
  "name": "slamhaus-lang-support",
  "displayName": "Slamhaus",
  "description": "Syntax highlighting and editor support for Slamhaus - a Markdown-based site generator.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "slamhaus",
      "aliases": ["Slamhaus", "slamhaus"],
      "extensions": [".md"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "slamhaus",
      "scopeName": "source.slamhaus",
      "path": "./syntaxes/slamhaus.tmLanguage.json"
    }]
  }
}
